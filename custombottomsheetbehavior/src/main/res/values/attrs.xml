<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ScrollLinearLayout">
        <attr name="content_view_id" format = "integer"/>
        <attr name="blank_view_id" format="integer"/>
    </declare-styleable>

    <declare-styleable name="ScrollLinearLayout2">
        <attr name="content_view_id2" format = "integer"/>
        <attr name="blank_view_id2" format="integer"/>
    </declare-styleable>
    <declare-styleable name="CustomBottomSheetBehavior">
        <attr name="anchorPoint" format="dimension"/>
        <attr name="defaultState" format="enum">
            <enum name="stateAnchorPoint" value="3" />
            <enum name="stateExpanded" value="4"/>
            <enum name="stateCollapsed" value="5"/>
            <enum name="stateHidden" value="6"/>
        </attr>
    </declare-styleable>
</resources>